<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Entity Document Query Deferred Request Secured WSDL
  -->
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
        xmlns="http://schemas.xmlsoap.org/wsdl/"
        xmlns:tns="urn:gov:hhs:fha:nhinc:entitydocquerydeferredrequestsecured"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:query="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0"
        xmlns:nccommonentity="urn:gov:hhs:fha:nhinc:common:nhinccommonentity"
        xmlns:dq="http://www.hhs.gov/healthit/nhin"
        xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
        xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
        xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702"
        xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl"
        name="EntityDocQueryDeferredRequestSecured"
        targetNamespace="urn:gov:hhs:fha:nhinc:entitydocquerydeferredrequestsecured">
    <documentation>Entity Document Query Deferred Request Secured</documentation>
    <types>
        <xsd:schema>
            <xsd:import namespace="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" schemaLocation="classpath:schemas/ebRS/query.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonentity" schemaLocation="classpath:schemas/nhinc/common/NhincCommonEntity.xsd"/>
            <xsd:import namespace="http://www.hhs.gov/healthit/nhin" schemaLocation="classpath:schemas/nhin/QueryDocument.xsd"/>  
        </xsd:schema>
    </types>
    <message name="RespondingGateway_CrossGatewayQueryRequestSecuredMessage">
        <part name="RespondingGateway_CrossGatewayQueryRequest" element="nccommonentity:RespondingGateway_CrossGatewayQuerySecuredRequest"/>
    </message>
    <message name="AcknowledgementMessage">
        <part name="RespondingGateway_CrossGatewayQueryResponse" element="dq:DocQueryAcknowledgement"/>
    </message>
    <portType name="EntityDocQueryDeferredRequestSecuredPortType">
        <operation name="RespondingGateway_CrossGatewayQuery">
            <input name="RespondingGateway_CrossGatewayQueryRequest" message="tns:RespondingGateway_CrossGatewayQueryRequestSecuredMessage"
                    wsaw:Action="tns:RespondingGateway_CrossGatewayQueryRequest"/>
            <output name="AcknowledgementMessage" message="tns:AcknowledgementMessage"
                    wsaw:Action="tns:RespondingGateway_CrossGatewayQueryRequestAck" />
        </operation>
    </portType>
    <binding name="EntityDocQueryDeferredRequestSecuredBindingSoap" type="tns:EntityDocQueryDeferredRequestSecuredPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsp:PolicyReference URI="#RespondingGateway_Query_Binding_SoapPolicy"/>
        <operation name="RespondingGateway_CrossGatewayQuery">
            <soap:operation soapAction="tns:RespondingGateway_CrossGatewayQueryRequest"/>
            <input name="RespondingGateway_CrossGatewayQueryRequest">
                <soap:body use="literal"/>
                 <wsp:PolicyReference URI="#Entity_DocQueryDeferred_Binding_Soap_Input_Policy"/>
            </input>
            <output name="AcknowledgementMessage">
                <soap:body use="literal"/>
                <wsp:PolicyReference URI="#Entity_DocQueryDeferred_Binding_Soap_Output_Policy"/>
            </output>
        </operation>
    </binding>
    <service name="EntityDocQueryDeferredRequestSecured">
        <port name="EntityDocQueryDeferredRequestSecuredPortSoap" binding="tns:EntityDocQueryDeferredRequestSecuredBindingSoap">
            <soap:address location="https://localhost:${HttpsDefaultPort}/NhinConnect/EntityDocQueryDeferredRequestSecured"/>
        </port>
    </service>
    <wsp:Policy wsu:Id="RespondingGateway_Query_Binding_SoapPolicy">
        <wsp:ExactlyOne>
            <wsp:All>
                <sp:TransportBinding>
                    <wsp:Policy>
                        <sp:TransportToken>
                            <wsp:Policy>
                                <sp:HttpsToken>
                                    <wsp:Policy>
                                        <sp:RequireClientCertificate/>
                                    </wsp:Policy>
                                </sp:HttpsToken>
                            </wsp:Policy>
                        </sp:TransportToken>
                        <sp:Layout>
                            <wsp:Policy>
                                <sp:Lax/>
                            </wsp:Policy>
                        </sp:Layout>
                        <sp:IncludeTimestamp/>
                        <sp:AlgorithmSuite>
                          <wsp:Policy>
                            <wsp:ExactlyOne>
                              <sp:Basic256 />
                              <sp:Basic192 />
                              <sp:Basic128 />
                              <sp:TripleDes />
                              <sp:Basic256Rsa15 />
                              <sp:Basic192Rsa15 />
                              <sp:Basic128Rsa15 />
                              <sp:TripleDesRsa15 />
                              <sp:Basic256Sha256 />
                              <sp:Basic192Sha256 />
                              <sp:Basic128Sha256 />
                              <sp:TripleDesSha256 />
                              <sp:Basic256Sha256Rsa15 />
                              <sp:Basic192Sha256Rsa15 />
                              <sp:Basic128Sha256Rsa15 />
                              <sp:TripleDesSha256Rsa15 />
                            </wsp:ExactlyOne>
                          </wsp:Policy>
                        </sp:AlgorithmSuite>
                    </wsp:Policy>
                </sp:TransportBinding>
                <sp:EndorsingSupportingTokens>
                    <wsp:Policy>
                        <sp:SamlToken sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
                            <wsp:Policy>
                                <sp:WssSamlV20Token11/>
                            </wsp:Policy>
                        </sp:SamlToken>
                    </wsp:Policy>
                </sp:EndorsingSupportingTokens>
                <sp:Wss11>
                    <wsp:Policy>
                        <sp:MustSupportRefKeyIdentifier/>
                        <sp:MustSupportRefIssuerSerial/>
                        <sp:RequireSignatureConfirmation/>
                    </wsp:Policy>
                </sp:Wss11>
				<wsaw:UsingAddressing />
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="Entity_DocQueryDeferred_Binding_Soap_Input_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
      </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="Entity_DocQueryDeferred_Binding_Soap_Output_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
      </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
</definitions>
