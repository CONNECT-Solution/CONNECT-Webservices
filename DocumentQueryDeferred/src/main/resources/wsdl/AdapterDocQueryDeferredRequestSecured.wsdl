<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Adapter Document Query Deferred Request Secured WSDL
  -->
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
 xmlns="http://schemas.xmlsoap.org/wsdl/"
 xmlns:tns="urn:gov:hhs:fha:nhinc:adapterdocquerydeferredrequestsecured" 
 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 xmlns:query="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" 
 xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl" 
 xmlns:wsaws="http://www.w3.org/2005/08/addressing" 
 xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" 
 xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" 
 xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy" 
 xmlns:sc="http://schemas.sun.com/2006/03/wss/server" 
 xmlns:wspp="http://java.sun.com/xml/ns/wsit/policy" 
 xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" 
 xmlns:vprop="http://docs.oasis-open.org/wsbpel/2.0/varprop" 
 xmlns:sxnmp="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/NMProperty" 
 xmlns:dq="http://www.hhs.gov/healthit/nhin" 
 xmlns:nccommonadapter="urn:gov:hhs:fha:nhinc:common:nhinccommonadapter" 
 name="AdapterDocQueryDeferredRequestSecured" 
 targetNamespace="urn:gov:hhs:fha:nhinc:adapterdocquerydeferredrequestsecured">
    <documentation>Adapter Document Query Deferred Request Secured</documentation>
    <types>
        <xsd:schema>
            <xsd:import namespace="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" schemaLocation="classpath:schemas/ebRS/query.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:gateway:samltokendata" schemaLocation="classpath:schemas/nhinc/gateway/SamlTokenData.xsd"/>
            <xsd:import namespace="http://www.hhs.gov/healthit/nhin" schemaLocation="classpath:schemas/nhin/QueryDocument.xsd"/>  
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonadapter" schemaLocation="classpath:schemas/nhinc/common/NhincCommonAdapter.xsd"/>
        </xsd:schema>
    </types>
    <message name="RespondingGateway_CrossGatewayQueryRequestMessage">
        <part name="body" element="nccommonadapter:RespondingGateway_CrossGatewayQuerySecureRequest"/>
    </message>
    <message name="RespondingGateway_CrossGatewayQueryResponseMessage">
        <part name="body" element="dq:DocQueryAcknowledgement"/>
    </message>
    <portType name="AdapterDocQueryDeferredRequestSecuredPortType">
        <operation name="RespondingGateway_CrossGatewayQuery">
            <input name="RespondingGateway_CrossGatewayQueryRequest" message="tns:RespondingGateway_CrossGatewayQueryRequestMessage" wsaw:Action="tns:RespondingGateway_CrossGatewayQuery"/>
            <output name="RespondingGateway_CrossGatewayQueryResponse" message="tns:RespondingGateway_CrossGatewayQueryResponseMessage" wsaw:Action="tns:RespondingGateway_CrossGatewayQueryResponse"/>
        </operation>
    </portType>
    <binding name="AdapterDocQueryDeferredRequestSecuredBindingSoap" type="tns:AdapterDocQueryDeferredRequestSecuredPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsp:PolicyReference URI="#RespondingGateway_Query_Binding_SoapPolicy"/>
        <operation name="RespondingGateway_CrossGatewayQuery">
            <soap:operation soapAction="tns:RespondingGateway_CrossGatewayQuery"/>
            <input name="RespondingGateway_CrossGatewayQueryRequest">
                <soap:body use="literal"/>
                <wsp:PolicyReference URI="#RespondingGateway_Query_Binding_Soap_Input_Policy"/>
            </input>
            <output name="RespondingGateway_CrossGatewayQueryResponse">
                <soap:body use="literal"/>
                <wsp:PolicyReference URI="#RespondingGateway_Query_Binding_Soap_Output_Policy"/>
            </output>
        </operation>
    </binding>
    <service name="AdapterDocQueryDeferredRequestSecured">
        <port name="AdapterDocQueryDeferredRequestSecuredPortSoap" binding="tns:AdapterDocQueryDeferredRequestSecuredBindingSoap">
            <soap:address location="https://localhost:${HttpsDefaultPort}/NhinConnect/AdapterDocQueryDeferredRequestSecured"/>
        </port>
    </service>

    <wsp:Policy wsu:Id="RespondingGateway_Query_Binding_SoapPolicy">
        <wsp:ExactlyOne>
            <wsp:All>
                <sp:TransportBinding>
                    <wsp:Policy>
                        <sp:TransportToken>
                            <wsp:Policy>
                                <sp:HttpsToken>
                                    <wsp:Policy>
                                        <sp:RequireClientCertificate/>
                                    </wsp:Policy>
                                </sp:HttpsToken>
                            </wsp:Policy>
                        </sp:TransportToken>
                        <sp:Layout>
                            <wsp:Policy>
                                <sp:Lax/>
                            </wsp:Policy>
                        </sp:Layout>
                        <sp:IncludeTimestamp/>
                        <sp:AlgorithmSuite>
                          <wsp:Policy>
                            <wsp:ExactlyOne>
                              <sp:Basic256 />
                              <sp:Basic192 />
                              <sp:Basic128 />
                              <sp:TripleDes />
                              <sp:Basic256Rsa15 />
                              <sp:Basic192Rsa15 />
                              <sp:Basic128Rsa15 />
                              <sp:TripleDesRsa15 />
                              <sp:Basic256Sha256 />
                              <sp:Basic192Sha256 />
                              <sp:Basic128Sha256 />
                              <sp:TripleDesSha256 />
                              <sp:Basic256Sha256Rsa15 />
                              <sp:Basic192Sha256Rsa15 />
                              <sp:Basic128Sha256Rsa15 />
                              <sp:TripleDesSha256Rsa15 />
                            </wsp:ExactlyOne>
                          </wsp:Policy>
                        </sp:AlgorithmSuite>
                    </wsp:Policy>
                </sp:TransportBinding>
                <sp:EndorsingSupportingTokens>
                    <wsp:Policy>
                        <sp:SamlToken sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
                            <wsp:Policy>
                                <sp:WssSamlV20Token11/>
                            </wsp:Policy>
                        </sp:SamlToken>
                    </wsp:Policy>
                </sp:EndorsingSupportingTokens>
                <sp:Wss11>
                    <wsp:Policy>
                        <sp:MustSupportRefKeyIdentifier/>
                        <sp:MustSupportRefIssuerSerial/>
                        <sp:RequireSignatureConfirmation/>
                    </wsp:Policy>
                </sp:Wss11>
				<wsaw:UsingAddressing />
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="RespondingGateway_Query_Binding_Soap_Input_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="RespondingGateway_Query_Binding_Soap_Output_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
</definitions>
